<Filters>
    <Filter id='544' name='Default' nodeType='group'>
        <Filter id='553' isDefault='true' name='In Maintenance' nodeType='filter'
                query='manager: * - manager:*Watch suppressescl:Maintenance'/>
        <Filter id='552' isDefault='true' name='All Events' nodeType='filter' query='id:*'/>
        <Filter id='554' isDefault='true' name='Escalated' nodeType='filter'
