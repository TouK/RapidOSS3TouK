<Model ClassName="Customer">
	<Properties>
		<Property  name="name" type="string" datasource="RCMDB" blank="false" lazy="false" />
		<Property  name="accountmanager" type="string" datasource="AccountDBDS" nameInDs="manager" lazy="false" />
	</Properties>
	<Datasources>
		<Datasource name="RCMDB" master="true">
			<Key propertyName="name" />
		</Datasource>
		<Datasource name="AccountDBDS" >
			<Key propertyName="name" nameinDS="code" />
		</Datasource>
	</Datasources>	
	<Relations>
		<Relation name="services" relatedTo="Service" />
	</Relations>
</Model>
