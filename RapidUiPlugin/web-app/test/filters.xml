        <Filter id='547' isDefault='true' name='Major Events' nodeType='filter' query='severity:Major'/>
        <Filter id='551' isDefault='true' name='Clear Events' nodeType='filter' query='severity:Clear'/>
        <Filter id='550' isDefault='true' name='Indeterminate Events' nodeType='filter' query='severity:Indeterminate'/>
        <Filter id='548' isDefault='true' name='Minor Events' nodeType='filter' query='severity:Minor'/>
        <Filter id='546' isDefault='true' name='Critical Events' nodeType='filter' query='severity:Critical'/>
    </Filter>
</Filters>