<Model ClassName="Service">
	<Properties>
		<Property  name="name" type="string" datasource="RCMDB" blank="false" lazy="false" />
		<Property  name="manager" type="string" datasource="RCMDB" lazy="false" />
		<Property  name="slaLevel" type="string" datasource="RCMDB" lazy="false" default="standard"/>
		<Property  name="status" type="string" datasource="RCMDB" />
	</Properties>
	<Datasources>
		<Datasource name="RCMDB" master="true" >
			<Key propertyName="name" />
		</Datasource>
	</Datasources>
	<Relations>
		<Relation name="customers" relatedTo="Customer" />
		<Relation name="resources" relatedTo="Resource" />
		<Relation name="tickets" relatedTo="Ticket" datasource="TicketDS"/>
	</Relations>	
</Model>
